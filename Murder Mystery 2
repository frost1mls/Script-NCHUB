local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local LP = Players.LocalPlayer

-- Cores profissionais
local CORES = {
    primary = Color3.fromRGB(100, 150, 255),
    success = Color3.fromRGB(75, 255, 100),
    danger = Color3.fromRGB(255, 75, 75),
    warning = Color3.fromRGB(255, 200, 75),
    murder = Color3.fromRGB(255, 50, 50),
    sheriff = Color3.fromRGB(50, 150, 255),
    innocent = Color3.fromRGB(75, 255, 100),
    gun = Color3.fromRGB(255, 255, 50)
}

-- ============================================
-- SE√á√ÉO 1: FARM AFK PROFISSIONAL
-- ============================================

local FarmAFK = {}
FarmAFK.__index = FarmAFK

function FarmAFK.new()
    local self = setmetatable({}, FarmAFK)
    
    self.isRunning = false
    self.coinsCollected = 0
    self.startTime = nil
    self.Char = LP.Character or LP.CharacterAdded:Wait()
    self.HRP = self.Char:WaitForChild("HumanoidRootPart")
    self.Humanoid = self.Char:WaitForChild("Humanoid")
    
    self:CreateUI()
    self:SetupEvents()
    self:StartLoop()
    
    return self
end

function FarmAFK:CreateUI()
    -- Frame principal do Farm
    self.frame = Instance.new("Frame")
    self.frame.Name = "FarmFrame"
    self.frame.Size = UDim2.new(1, -20, 0, 160)
    self.frame.Position = UDim2.new(0, 10, 0, 10)
    self.frame.BackgroundColor3 = Color3.fromRGB(30, 30, 38)
    self.frame.BackgroundTransparency = 0.3
    self.frame.BorderSizePixel = 0
    self.frame.Parent = contentArea
    
    local frameCorner = Instance.new("UICorner")
    frameCorner.CornerRadius = UDim.new(0, 12)
    frameCorner.Parent = self.frame
    
    -- T√≠tulo com √≠cone
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -20, 0, 35)
    title.Position = UDim2.new(0, 10, 0, 10)
    title.BackgroundTransparency = 1
    title.Text = "üåæ FARM AFK PREMIUM"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 18
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.Parent = self.frame
    
    -- Status
    self.statusLabel = Instance.new("TextLabel")
    self.statusLabel.Size = UDim2.new(1, -20, 0, 25)
    self.statusLabel.Position = UDim2.new(0, 10, 0, 45)
    self.statusLabel.BackgroundTransparency = 1
    self.statusLabel.Text = "‚ö™ Status: Parado"
    self.statusLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
    self.statusLabel.Font = Enum.Font.Gotham
    self.statusLabel.TextSize = 14
    self.statusLabel.TextXAlignment = Enum.TextXAlignment.Left
    self.statusLabel.Parent = self.frame
    
    -- Contador
    self.counterLabel = Instance.new("TextLabel")
    self.counterLabel.Size = UDim2.new(1, -20, 0, 25)
    self.counterLabel.Position = UDim2.new(0, 10, 0, 70)
    self.counterLabel.BackgroundTransparency = 1
    self.counterLabel.Text = "üí∞ Moedas: 0"
    self.counterLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    self.counterLabel.Font = Enum.Font.Gotham
    self.counterLabel.TextSize = 14
    self.counterLabel.TextXAlignment = Enum.TextXAlignment.Left
    self.counterLabel.Parent = self.frame
    
    -- Bot√£o
    self.toggleBtn = Instance.new("TextButton")
    self.toggleBtn.Size = UDim2.new(0, 150, 0, 35)
    self.toggleBtn.Position = UDim2.new(0.5, -75, 0, 105)
    self.toggleBtn.BackgroundColor3 = CORES.success
    self.toggleBtn.Text = "INICIAR FARM"
    self.toggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    self.toggleBtn.Font = Enum.Font.GothamBold
    self.toggleBtn.TextSize = 14
    self.toggleBtn.Parent = self.frame
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 8)
    btnCorner.Parent = self.toggleBtn
    
    -- Bind do bot√£o
    self.toggleBtn.MouseButton1Click:Connect(function()
        self:Toggle()
    end)
end

function FarmAFK:Toggle()
    self.isRunning = not self.isRunning
    
    if self.isRunning then
        self.startTime = os.clock()
        self.toggleBtn.BackgroundColor3 = CORES.danger
        self.toggleBtn.Text = "PARAR FARM"
        self.statusLabel.Text = "üü¢ Status: Coletando moedas..."
        self.statusLabel.TextColor3 = CORES.success
    else
        self.toggleBtn.BackgroundColor3 = CORES.success
        self.toggleBtn.Text = "INICIAR FARM"
        self.statusLabel.Text = "‚ö™ Status: Parado"
        self.statusLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
    end
end

function FarmAFK:FindNearestCoin()
    local mapa = nil
    for _, obj in ipairs(workspace:GetChildren()) do
        if obj:FindFirstChild("CoinContainer") or obj:FindFirstChild("Coins") then
            mapa = obj
            break
        end
    end
    
    if not mapa then return nil end
    
    local container = mapa:FindFirstChild("CoinContainer") or mapa:FindFirstChild("Coins")
    if not container then return nil end
    
    local nearest = nil
    local minDist = math.huge
    
    for _, coin in ipairs(container:GetChildren()) do
        local visual = coin:FindFirstChild("CoinVisual")
        if visual and not visual:GetAttribute("Collected") then
            local dist = (self.HRP.Position - coin.Position).Magnitude
            if dist < minDist then
                nearest = coin
                minDist = dist
            end
        end
    end
    
    return nearest
end

function FarmAFK:MoveToCoin(coin)
    if not coin or not self.HRP or not self.Humanoid then return false end
    
    self.Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
    
    local distance = (self.HRP.Position - coin.Position).Magnitude
    local travelTime = distance / 35
    
    local tween = TweenService:Create(
        self.HRP, 
        TweenInfo.new(travelTime, Enum.EasingStyle.Linear),
        {CFrame = coin.CFrame}
    )
    
    tween:Play()
    tween.Completed:Wait()
    
    self.Humanoid:ChangeState(Enum.HumanoidStateType.Running)
    return true
end

function FarmAFK:SetupEvents()
    LP.CharacterAdded:Connect(function(newChar)
        self.Char = newChar
        self.HRP = newChar:WaitForChild("HumanoidRootPart")
        self.Humanoid = newChar:WaitForChild("Humanoid")
    end)
end

function FarmAFK:StartLoop()
    spawn(function()
        while true do
            if self.isRunning then
                if LP.Character and LP.Character:FindFirstChild("Humanoid") and LP.Character.Humanoid.Health > 0 then
                    local coin = self:FindNearestCoin()
                    
                    if coin then
                        local success = self:MoveToCoin(coin)
                        
                        if success then
                            local visual = coin:FindFirstChild("CoinVisual")
                            if visual then
                                repeat
                                    task.wait(0.1)
                                until not visual.Parent or visual:GetAttribute("Collected")
                                
                                self.coinsCollected = self.coinsCollected + 1
                                self.counterLabel.Text = "üí∞ Moedas: " .. self.coinsCollected
                            end
                        end
                    else
                        self.statusLabel.Text = "‚è≥ Status: Procurando moedas..."
                        task.wait(0.5)
                    end
                else
                    self.statusLabel.Text = "üíÄ Status: Morto - Aguardando..."
                    self.statusLabel.TextColor3 = CORES.danger
                    task.wait(1)
                end
            else
                task.wait(0.1)
            end
        end
    end)
end

